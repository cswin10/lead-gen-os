# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Service Role Key - Required for admin operations (creating users, etc.)
# Get this from your Supabase dashboard -> Settings -> API
# WARNING: Keep this secret! Never commit to version control
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Sentry Error Tracking
# Get your DSN from https://sentry.io/ after creating a project
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
SENTRY_ORG=your_sentry_organization
SENTRY_PROJECT=your_sentry_project_name
# Auth token for uploading source maps (get from Sentry -> Settings -> Auth Tokens)
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here

# Twilio Configuration (for calling features)
# Get these from your Twilio Console: https://console.twilio.com
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
# Optional: For advanced features (if not set, will use Account SID and Auth Token)
TWILIO_API_KEY=your_twilio_api_key
TWILIO_API_SECRET=your_twilio_api_secret
# TwiML App SID for voice calling (create in Twilio Console under Voice > TwiML Apps)
TWILIO_TWIML_APP_SID=your_twiml_app_sid

# Application URL (required for Twilio callbacks)
NEXT_PUBLIC_APP_URL=http://localhost:3000
